@model Concert.Data.Entities.Ticket
@{
    ViewData["Title"] = "Create";
}

<h1>Consulta ID Boleto</h1>

<h4>Boletos</h4>
<hr />
<div class="row">
    <div class="col-md-12">
        <form asp-action="ConsultIdTicket" enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />

            <div class="row">
                <div class="col-md-6">
                    <div class="form-group">
                        <label asp-for="Id" class="control-label"></label>
                        <input asp-for="Id" class="form-control" />
                        <span asp-validation-for="Id" class="text-danger"></span>
                    </div>
                </div>
            </div>


            <div class="form-group mt-2">
                <input type="submit" value="Consultar" class="btn btn-outline-primary" />
                <a asp-action="Index" class="btn btn-outline-success">Regresar</a>
            </div>
        </form>
    </div>
</div>